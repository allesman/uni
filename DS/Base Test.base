views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.contains("DS")
    order:
      - file.name
      - file.folder
    sort:
      - property: file.name
        direction: DESC
  - type: cards
    name: View
    cardSize: 220
    image: file.file
    imageFit: ""
    imageAspectRatio: 0.8
